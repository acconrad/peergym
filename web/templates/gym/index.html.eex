<div class="map-container">
  <div id="map-canvas"></div>
</div>

<%= for gym <- @gyms do %>
  <%= link "Show", to: gym_path(@conn, :show, gym), class: "btn btn-default btn-xs" %>
  <%= link "Edit", to: gym_path(@conn, :edit, gym), class: "btn btn-default btn-xs" %>
  <%= link "Delete", to: gym_path(@conn, :delete, gym), method: :delete, class: "btn btn-danger btn-xs" %>
<% end %>

<%= link "New gym", to: gym_path(@conn, :new) %>
