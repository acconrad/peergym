<li class="gym-item" data-latitude="<%= @item.latitude %>" data-longitude="<%= @item.longitude %>">
  <h3><%= link @item.name, to: @to_show %></h3>
  <p><%= @item.address %> / <strong><%= @item.hours %></strong></p>
  <%= if @admin do %>
    <div class="well well-sm">
      <strong>Admin:</strong>
      <%= link 'Edit', to: @to_edit %> | <%= link 'Delete', to: @to_delete, method: :delete, data: [confirm: "Are you sure?"] %>
    </div>
  <% end %>
</li>
