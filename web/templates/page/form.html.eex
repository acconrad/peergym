<%= form_for @changeset, @action, [id: "gym-search-form",class: "form-inline", name: :search, method: "get"], fn f -> %>
  <%= hidden_input f, :place %>
  <div class="form-group">
    <%= text_input f, :location, class: "form-control", placeholder: "Enter a location" %>
  </div>
  <%= submit "Find a Gym", class: "btn btn-info" %>
<% end %>
